<div class="c-logo-header">
  <div class="c-logo-header__logo">
    {% render '@logo-vd' %}
  </div>
  <div class="c-logo-header__text">
    {% for item in text %}
      {% if loop.last %}
        {{ item }}
      {%- else -%}
        {{ item }}<br>
      {% endif %}
    {% endfor %}
  </div>
</div>

